{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "jsx-a11y/href-no-hash": "off",
        "react/jsx-filename-extension": "off",
        "import/extensions": "off",
        "react/forbid-prop-types": "off"
    },
    "parser": "babel-eslint"
}